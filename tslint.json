{
  "extends": [
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "import-name": false,
    "variable-name": false,
    "function-name": false,
    "max-line-length": [ true, 140 ],
    "align": [true, "statements", "parameters"]
  }
}
